section "game/modules/tome/data/damage_types.lua"

t("%s(%d warded)#LAST#", "%s(%d 被守护)#LAST#", "tformat")
t("#Source# strikes #Target# in the darkness (%+d%%%%%%%% damage).", "#Source#在黑暗中打击了#Target#(%+d%%%%%%%% 伤害)!", "delayedLogMessage")
